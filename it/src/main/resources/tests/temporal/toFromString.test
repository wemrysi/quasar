{
    "name": "convert dates to/from strings",
    "backends": {
        "lwc_local":      "ignoreFieldOrder",
        "mimir":          "ignoreFieldOrder"
    },
    "data": "../days.data",
    "query": "select date(substring(str, 0, 10)) as d, time(substring(str, 11, 8)) as t, timestamp(str) as ts, to_string(ts) as s from `../days.data`",
    "predicate": "exactly",
    "ignoreResultOrder": true,
    "expected": [
        { "d": { "$localdate": "2014-08-17" }, "t": { "$localtime": "06:00:00.000000000" }, "ts": { "$offsetdatetime": "2014-08-17T06:00:00.000000000Z" }, "s": "2014-08-17T06:00Z" },
        { "d": { "$localdate": "2014-08-18" }, "t": { "$localtime": "07:00:00.000000000" }, "ts": { "$offsetdatetime": "2014-08-18T07:00:00.000000000Z" }, "s": "2014-08-18T07:00Z" },
        { "d": { "$localdate": "2014-08-19" }, "t": { "$localtime": "08:00:00.000000000" }, "ts": { "$offsetdatetime": "2014-08-19T08:00:00.000000000Z" }, "s": "2014-08-19T08:00Z" },
        { "d": { "$localdate": "2014-08-20" }, "t": { "$localtime": "09:00:00.000000000" }, "ts": { "$offsetdatetime": "2014-08-20T09:00:00.000000000Z" }, "s": "2014-08-20T09:00Z" },
        { "d": { "$localdate": "2014-08-21" }, "t": { "$localtime": "10:00:00.000000000" }, "ts": { "$offsetdatetime": "2014-08-21T10:00:00.000000000Z" }, "s": "2014-08-21T10:00Z" },
        { "d": { "$localdate": "2014-08-22" }, "t": { "$localtime": "11:00:00.000000000" }, "ts": { "$offsetdatetime": "2014-08-22T11:00:00.000000000Z" }, "s": "2014-08-22T11:00Z" },
        { "d": { "$localdate": "2014-08-23" }, "t": { "$localtime": "12:00:00.000000000" }, "ts": { "$offsetdatetime": "2014-08-23T12:00:00.000000000Z" }, "s": "2014-08-23T12:00Z" },
        { "d": { "$localdate": "1969-12-31" }, "t": { "$localtime": "23:59:59.000000000" }, "ts": { "$offsetdatetime": "1969-12-31T23:59:59.000000000Z" }, "s": "1969-12-31T23:59:59Z" },
        { "d": { "$localdate": "1653-02-10" }, "t": { "$localtime": "01:13:20.000000000" }, "ts": { "$offsetdatetime": "1653-02-10T01:13:20.000000000Z" }, "s": "1653-02-10T01:13:20Z" }]
}
